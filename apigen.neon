source:
    - src/
destination: build/api
exclude:
    - "*/vendor/*"
    - "*/test/*"
title: Filter
groups: auto
autocomplete:
    - classes
    - constants
    - functions
accessLevels:
    - public
    - protected
    - private
internal: yes
php: no
tree: yes
deprecated: yes
todo: yes
sourceCode: yes
report: build/api/apigen.log
wipeout: yes
quiet: no
progressbar: no
colors: no
debug: no
